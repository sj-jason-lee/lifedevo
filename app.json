{
  "expo": {
    "name": "Pasture",
    "slug": "pasture",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F4F7F2"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pasture.app",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F4F7F2"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.pasture.app",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.782405004129-ve2n6f9smu6811190cguk7u4s9p930fn"
        }
      ],
      "./plugins/withShortCmakePath.js"
    ],
    "extra": {
      "eas": {
        "projectId": "4f7c89b0-3b9b-4782-adaf-bf82665acac0"
      }
    }
  }
}
